    <Company>Bartosz Jarmuż</Company>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>C:\Users\bjarmuz\source\repos\RandomFriendlyNameGenerator\src\RandomFriendlyNameGenerator\RandomFriendlyNameGenerator.xml</DocumentationFile>
